{
    "name": "cine.pwa",
    "version": "4.0.1",
    "description": "React-PWA",
    "private": true,
    "scripts": {
        "start":
            "NODE_ENV=development webpack-dev-server --config webpack.dev.js",
        "format":
            "prettier --config .prettierrc.js --write \"src/**/*.{js,jsx,css,less}\"",
        "dev": "NODE_ENV=development webpack --config webpack.dev.js",
        "prod":
            "NODE_ENV=production webpack --progress --config webpack.prod.js",
